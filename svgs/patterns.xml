<?xml version="1.0" encoding="UTF-8"?>
<!--
    SVG files listed in here will have to adhere to some conventions:
        - the SVG tag must specify viewBox
        - the SVG width and height should use pixels as measurement unit
            - if other unit is used, may not appear as expected
        - the style tag should be a direct child of the svg tag
            - CSS properties that are desired to be attached to input should 
              be assigned to a CSS variable in the ':root' selector at the top
              of your <style> tag
                - the :root selector should only hold CSS variables
                - each CSS variable should be on its own line
                - colors should be in hex format and be full 6 characters (e.g. not 3 hex format)
                - for now numbers should use pixels as their unit
 -->
<patterns>
    <pattern>
        <file>morphing-diamonds.svg</file>
    </pattern>
    <pattern>
        <file>aztec.svg</file>
    </pattern>
    <pattern>
        <file>hlines.svg</file>
    </pattern>
	<pattern>
		<file>vlines.svg</file>
    </pattern>
    <pattern>
        <file>simplegrid.svg</file>
    </pattern>
    <pattern>
        <file>xbox.svg</file>
    </pattern>
</patterns>
